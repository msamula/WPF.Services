<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net10.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ImplicitUsings>enable</ImplicitUsings>
	<TreatWarningsAsErrors>true</TreatWarningsAsErrors>
  </PropertyGroup>

	<PropertyGroup>
		<Authors>Michael Samula</Authors>
		<Product>MSamula.WPFServices</Product>
		<Description>A collection of common, reusable services for WPF applications.</Description>
		<RepositoryUrl>https://github.com/msamula/WPF.Services.git</RepositoryUrl>
		<PackageProjectUrl>https://github.com/msamula/WPF.Services</PackageProjectUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
		<PackageTags>WPF;Services;MVVM;OpenFileDialog;OpenFolderDialog;SaveFileDialog</PackageTags>
	</PropertyGroup>

	<PropertyGroup>
		<AssemblyVersion>0.1.0.0</AssemblyVersion>
		<FileVersion>0.1.0.0</FileVersion>
		<Version>0.1.0</Version>
		<InformationalVersion>0.1.0</InformationalVersion>
	</PropertyGroup>

</Project>
